generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  //url      = env("DATABASE_URL") // ancienne version prisma
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  username  String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Appointments {
  id String @id @default(cuid())
  firstName String
  lastName String
  email String
  date DateTime
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}