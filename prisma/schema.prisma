generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Appointments {
  id String @id @default(cuid())
  firstName String
  lastName String
  email String
  date DateTime
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id String @id @default(cuid())
  username String
  email String
  password String
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}